 1 file changed, 61 insertions(+)

diff --git a/GoogleAdsPlayground/AdViewController.swift b/GoogleAdsPlayground/AdViewController.swift
index 9e5834c..45c1744 100644
--- a/GoogleAdsPlayground/AdViewController.swift
+++ b/GoogleAdsPlayground/AdViewController.swift
@@ -6,8 +6,69 @@
 //  Copyright © 2018 Haoxin Li. All rights reserved.
 //
 
+import GoogleMobileAds
 import UIKit
 
 final class AdViewController: UIViewController {
+    
+    private lazy var adView: GADBannerView = {
+        let v = GADBannerView(adSize: kGADAdSizeBanner)
+        v.delegate = self
+        v.adSizeDelegate = self
+        v.rootViewController = self
+        v.adUnitID = "ca-app-pub-3940256099942544/2934735716"
+        v.bounds = CGRect(origin: .zero, size: CGSize(width: 320, height: 50))
+        return v
+    }()
+    
+    override func viewDidLoad() {
+        super.viewDidLoad()
+        view.addSubview(adView)
+        adView.center = view.center
+        adView.load(adRequest)
+    }
+}
+
+private extension AdViewController {
+    
+    var adRequest: GADRequest {
+        let request = GADRequest()
+        request.testDevices = [kGADSimulatorID]
+        return request
+    }
+}
+
+extension AdViewController: GADBannerViewDelegate {
+    
+    func adViewDidReceiveAd(_ bannerView: GADBannerView) {
+        print("\(#function)")
+    }
+    
+    func adView(_ bannerView: GADBannerView, didFailToReceiveAdWithError error: GADRequestError) {
+        print("\(#function) \(error.localizedDescription)")
+    }
+    
+    func adViewWillPresentScreen(_ bannerView: GADBannerView) {
+        print("\(#function)")
+    }
+    
+    func adViewWillDismissScreen(_ bannerView: GADBannerView) {
+        print("\(#function)")
+    }
+    
+    func adViewDidDismissScreen(_ bannerView: GADBannerView) {
+        print("\(#function)")
+    }
+    
+    func adViewWillLeaveApplication(_ bannerView: GADBannerView) {
+        print("\(#function)")
+    }
+}
 
+extension AdViewController: GADAdSizeDelegate {
+    
+    func adView(_ bannerView: GADBannerView, willChangeAdSizeTo size: GADAdSize) {
+        print("\(#function) \(size.size)")
+        bannerView.bounds = CGRect(origin: bannerView.bounds.origin, size: size.size)
+    }
 }
-- 
